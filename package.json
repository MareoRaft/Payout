{
  "name": "payout",
  "productName": "Payout",
  "version": "1.0.0",
  "description": "Pay out ERC20 tokens to many people at once.",
  "main": "build/index.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint src --color"
  },
  "repository": "https://github.com/MareoRaft/Payout",
  "keywords": [],
  "author": "Matthew Lancellotti",
  "license": "MIT",
  "config": {
    "forge": {
      "make_targets": {
        "win32": [
          "squirrel"
        ],
        "darwin": [
          "zip"
        ],
        "linux": [
          "deb",
          "rpm"
        ]
      },
      "electronPackagerConfig": {
        "packageManager": "npm",
        "icon": "source/assets/images/icon",
        "asar": true
      },
      "electronWinstallerConfig": {
        "name": "Payout"
      },
      "electronInstallerDebian": {},
      "electronInstallerRedhat": {},
      "github_repository": {
        "owner": "mareoraft",
        "name": "Payout"
      },
      "windowsStoreConfig": {
        "packageName": "",
        "name": "Payout"
      }
    }
  },
  "dependencies": {
    "bignumber.js": "^7.2.1",
    "check-types": "^7.4.0",
    "csv-parse": "^3.0.0",
    "d3": "^5.7.0",
    "datejs": "^1.0.0-rc3",
    "electron-compile": "^6.4.3",
    "electron-squirrel-startup": "^1.0.0",
    "electron-store": "^2.0.0",
    "electron-window-state": "^5.0.0",
    "jquery": "^3.3.1",
    "lodash": "^4.17.10",
    "node-machine-id": "^1.1.10",
    "os-locale": "^3.0.1",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.5",
    "send-tokens": "^2.0.2",
    "simple-encryptor": "^2.0.0"
  },
  "devDependencies": {
    "electron-forge": "^5.2.2",
    "electron-prebuilt-compile": "2.0.7",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^6.0.0",
    "gulp-for-compass": "0.0.2"
  }
}
